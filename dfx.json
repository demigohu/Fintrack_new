{
  "canisters": {
    "fintrack_backend": {
      "type": "rust",
      "package": "fintrack_backend",
      "candid": "fintrack_backend/fintrack_backend.did",
      "optimize": "cycles",
      "http": { "enabled": true },
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "src/declarations/fintrack_backend"
      }
    },

    "fintrack_frontend": {
      "type": "assets",
      "frontend": { "entrypoint": "fintrack_frontend/out/index.html" },
      "source": ["fintrack_frontend/out"]
    },

    "internet_identity": {
      "remote": {
        "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
        "id": { "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai" }
      },
      "frontend": {},
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "src/declarations/internet_identity"
      }
    },

    "ckbtc_minter": {
